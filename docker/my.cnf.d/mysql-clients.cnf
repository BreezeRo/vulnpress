#
# These groups are read by MariaDB command-line tools
# Use it for options that affect only one utility
#

[mysql]
# faster start of mysql but no tab completition
no-auto-rehash

[mysqld_safe]
nice = 0

[mysqldump]
quick
quote-names
max_allowed_packet = 16M

[isamchk]
key_buffer = 16M

[mysqlimport]

[mysqlshow]

[mysqlslap]

[mysql_upgrade]

[mysqladmin]

[mysqlbinlog]

[mysqlcheck]
